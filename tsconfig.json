{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "types": ["cypress", "node"],
    "target": "es2021",
    "strict": false,
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "sourceMap": true
  },
  "include": ["src/**/*.ts", "src/support/index.d.ts"]
}
