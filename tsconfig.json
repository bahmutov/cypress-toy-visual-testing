{
  "compilerOptions": {
    "moduleResolution": "node",
    "types": ["cypress"],
    "target": "es2016",
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "sourceMap": true
  },
  "include": ["src/**/*.ts", "src/support/index.d.ts"]
}
